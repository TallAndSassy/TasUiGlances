{
    "name": "tallandsassy/tasuiglances",
    "description": "Tall and Sassy UI functionality that isn't too complicated. Think 'flash' and fancy reveals.",
    "license": "mit",
    "authors": [
        {
            "name": "JJ Rohrer",
            "email": "jj.github@rohrer.org",
            "homepage": "https://www.eleganttechnologies/aboutme"
        }
    ],
    "homepage": "https://github.com/tallandsassy/tasuiglances",
    "keywords": ["Laravel", "TasUiGlances"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8"

    },
    "require": {
        "mattlibera/livewire-flash": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TallAndSassy\\TasUiGlances\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TallAndSassy\\TasUiGlances\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TallAndSassy\\TasUiGlances\\TasUiGlancesServiceProvider"
            ],
            "aliases": {
                "TasUiGlances": "TallAndSassy\\TasUiGlances\\Facades\\TasUiGlances"
            }
        }
    }
}
